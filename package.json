    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "ts-registry": "^1.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
